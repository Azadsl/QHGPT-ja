<!-- 烟雾效果部分 -->
<section class="fog" id="fogEffect">
  <figure class="absolute-bg"></figure>
  <div class="fog__container">
    <div class="fog__img fog__img--first-1"></div>
    <div class="fog__img fog__img--first-2"></div>
    <div class="fog__img fog__img--second-1"></div>
    <div class="fog__img fog__img--second-2"></div>
  </div>
</section>

<style>
body, html {
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  position: relative;
}

.absolute-bg {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
  height: 100%;
  width: 100%;
  background-position: 50%;
  background-repeat: no-repeat;
  background-size: cover;
  overflow: hidden;
}

.fog {
  position: fixed;
  top: -20vh; /* 更大的负值确保覆盖顶部 */
  left: 0;
  height: 140vh; /* 更大的高度确保覆盖 */
  width: 100%;
  background-color: transparent;
  z-index: -1;
  opacity: 0.7;
  pointer-events: none;
  display: none;
}

.fog__container {
  position: absolute;
  height: 140%; /* 增加高度 */
  width: 100%;
  overflow: hidden;
}

.fog__img {
  position: absolute;
  height: 140vh; /* 增加高度 */
  width: 300vw;
  will-change: transform;
  transform: translate3d(0, 0, 0);
  background-size: cover !important; /* 改为cover而不是contain */
}

.fog__img--first-1 {
  background: url("fog-1.png") repeat-x;
  background-position: center;
  animation: marquee1 120s linear infinite;
  left: 0;
  top: 0;
}

.fog__img--first-2 {
  background: url("fog-1.png") repeat-x;
  background-position: center;
  animation: marquee2 120s linear infinite;
  left: 0;
  top: 0;
}

.fog__img--second-1 {
  background: url("fog-2.png") repeat-x;
  background-position: center;
  animation: marquee1 80s linear infinite;
  left: 0;
  top: 0;
}

.fog__img--second-2 {
  background: url("fog-2.png") repeat-x;
  background-position: center;
  animation: marquee2 80s linear infinite;
  left: 0;
  top: 0;
}

@keyframes marquee1 {
  0% { transform: translate3d(0, 0, 0); }
  100% { transform: translate3d(-100%, 0, 0); }
}

@keyframes marquee2 {
  0% { transform: translate3d(100%, 0, 0); }
  100% { transform: translate3d(0, 0, 0); }
}

.toggle-container {
  position: fixed;
  top: 20px;
  right: 20px;
  z-index: 100;
  cursor: pointer;
}

.toggle-icon {
  display: block;
  width: 30px;
  height: 30px;
}

.fog-off-icon {
  display: none;
}

.toggle-container.active .fog-on-icon {
  display: none;
}

.toggle-container.active .fog-off-icon {
  display: block;
}

/* 移动设备专用样式 */
@media only screen and (max-width: 768px) {
  .fog {
    top: -30vh;
    height: 160vh;
  }
  
  .fog__img {
    height: 160vh;
  }
}
</style>
