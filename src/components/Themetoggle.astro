<script>
// 获取DOM元素
const fogEffect = document.getElementById('fogEffect');
const fogToggle = document.getElementById('fogToggle');
let fogEnabled = false;

// 预加载图片以提高性能
function preloadImages() {
  const imageUrls = ['fog-1.png', 'fog-2.png'];
  imageUrls.forEach(url => {
    const img = new Image();
    img.src = url;
  });
}

// 调整烟雾元素以完全覆盖屏幕
function adjustFogSize() {
  const vh = window.innerHeight * 0.01;
  document.documentElement.style.setProperty('--vh', `${vh}px`);
  
  // 根据设备高度调整烟雾覆盖
  if (window.innerWidth <= 768) {
    // 移动设备
    fogEffect.style.top = '-30vh';
    fogEffect.style.height = '160vh';
  } else {
    // 桌面设备
    fogEffect.style.top = '-20vh';
    fogEffect.style.height = '140vh';
  }
}

// 点击图标切换烟雾效果
fogToggle.addEventListener('click', function() {
  fogEnabled = !fogEnabled;
  if (fogEnabled) {
    fogEffect.style.display = 'block';
    fogToggle.classList.add('active');
    adjustFogSize(); // 调整尺寸确保覆盖
  } else {
    fogEffect.style.display = 'none';
    fogToggle.classList.remove('active');
  }
});

// 在页面加载和窗口尺寸变化时调整烟雾尺寸
window.addEventListener('load', function() {
  preloadImages();
  adjustFogSize();
});

window.addEventListener('resize', adjustFogSize);
</script>



